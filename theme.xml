        <Image id="fs_bot_position_background" image="images_prubeh_bg_video" lefttop="leftbottom" rightbottom="rightbottom"/>
        <Slider id="fs_position" points="(0,5),(600,5)" value="time" tooltiptext="" up="images_handle" down="images_handle" over="images_handle" lefttop="leftbottom" rightbottom="rightbottom">
          <SliderBackground id="fs_position_bg" image="images_slider_position_white" nbvert="601"/>
        </Slider>
      </Panel>
      <Panel id="fs_Left_panel" y="10" width="200" height="57" lefttop="leftbottom" rightbottom="leftbottom">
        <Button up="images_cross_n" down="images_cross_p" over="images_cross_h" action="vlc.quit()" id="fs_btn_close" x="17" y="16" lefttop="leftbottom" rightbottom="leftbottom"/>
        <Button up="images_min_n" down="images_min_p" over="images_min_h" action="vlc.fullscreen();vlc.minimize()" id="fs_btn_min" x="45" y="16" lefttop="leftbottom" rightbottom="leftbottom"/>
        <Checkbox state="pl_on.isActive" up1="images_playlist_btn_n" up2="images_playlist_btn_n" down1="images_playlist_btn_p" down2="images_playlist_btn_p" over1="images_playlist_btn_h" over2="images_playlist_btn_h" action1="vlc.fullscreen();playlist_switch.setLayout(pl_on)" action2="vlc.fullscreen();playlist_switch.setLayout(pl_off)" id="fs_btn_playlist_chbx" x="200" y="16" rightbottom="leftbottom"/>
        <Text text="$T" font="Sansation_Light_16" color="#f5f7fa" id="fs_Time_cur" x="74" y="20" width="60" lefttop="leftbottom" rightbottom="leftbottom"/>
        <Text text="/" font="Sansation_Light_16" color="#6f747b" id="fs_slash" x="130" y="20" width="10" lefttop="leftbottom" rightbottom="leftbottom"/>
        <Text text="$D" font="Sansation_Light_16" color="#6f747b" id="fs_Time_full" x="140" y="20" width="60" lefttop="leftbottom" rightbottom="leftbottom"/>
      </Panel>
      <Panel id="fs_Control_panel" x="200" y="10" width="200" height="57" lefttop="leftbottom" rightbottom="rightbottom" xkeepratio="true">
        <Checkbox state="vlc.isPlaying" up1="images_play_n" up2="images_pause_n" down1="images_play_p" down2="images_pause_p" over1="images_play_h" over2="images_pause_h" action1="vlc.play()" action2="vlc.pause()" id="fs_Play" x="79" y="11" lefttop="leftbottom" rightbottom="leftbottom"/>
        <Button up="images_prev_n" down="images_prev_p" over="images_prev_h" action="playlist.next()" id="fs_btn_prev" x="45" y="14" lefttop="leftbottom" rightbottom="leftbottom"/>
        <Button up="images_next_n" down="images_next_p" over="images_next_h" action="playlist.next()" id="fs_btn_next" x="120" y="14" lefttop="leftbottom" rightbottom="leftbottom"/>
      </Panel>
      <Panel id="fs_Volume_panel" x="430" y="24" width="160" height="30" lefttop="rightbottom" rightbottom="rightbottom">
        <Image id="fs_volume_background" x="30" y="10" image="images_volume_bg" lefttop="rightbottom" rightbottom="rightbottom"/>
        <Checkbox state="vlc.isMute" up1="images_volume_on" up2="images_volume_off" down1="images_volume_on_c" down2="images_volume_on_c" over1="images_volume_on_h" over2="images_volume_on_h" action1="vlc.mute()" action2="vlc.mute()" id="fs_volume_mute" y="2"/>
        <Slider id="fs_volume_slider" x="30" y="10" points="(0,3),(100,3)" value="volume" tooltiptext="" up="images_handle_volume" down="images_handle" over="images_handle_volume" lefttop="leftbottom" rightbottom="rightbottom">
          <SliderBackground id="fs_volume_back" image="images_slider_volume_white" nbvert="101"/>
        </Slider>
      </Panel>
    </Layout>
  </Window>
  <Window id="playlist_switch" x="1" y="1">
    <Layout id="pl_on" width="1" height="1">
    </Layout>
    <Layout id="pl_off" width="1" height="1">
    </Layout>
  </Window>

